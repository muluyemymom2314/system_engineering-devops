#!/usr/bin/env bash
# A Bash script that stops the 4-to_infinity_and_beyond process

# Find the PID of the running 4-to_infinity_and_beyond process
pid=$(pgrep -f "4-to_infinity_and_beyond")

# Check if a PID was found
if [ -n "$pid" ]; then
    # Send the default TERM signal to terminate the process
    kill "$pid"
fi
